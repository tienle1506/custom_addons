    <record id="hrm_quyen_dongdau_coquan" model="res.groups">
        <field name="name">Đóng dấu cơ quan</field>
        <field name="category_id" ref="module_hrm_kyso"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="hrm_quyen_kyso_kynhay" model="res.groups">
        <field name="name">Ký nháy</field>
        <field name="category_id" ref="module_hrm_kyso"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
</odoo>